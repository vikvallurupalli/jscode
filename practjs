function factorialize(num) {
    if (num == 0 || num === 1)
      return 1;
    for (var i = num - 1; i >= 1; i--) {
      num = (num* i);
    }
    return num;
  }


  function sumOfNnumbers(number){
      var returnvalue = 0;
      for(var i=number; i > 1; i--){
            returnvalue = returnvalue + i;
      }
      return returnvalue;
  }

  console.log(sumOfNnumbers(3));
